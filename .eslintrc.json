{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    // "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": 0,
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "func-names": 0,
        "no-undef": 0,
        "indent": 0
    }
}
